#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/keycloakify-storyboard-storybook/node_modules/.pnpm/keycloakify@11.3.19/node_modules/keycloakify/bin/node_modules:/home/user/keycloakify-storyboard-storybook/node_modules/.pnpm/keycloakify@11.3.19/node_modules/keycloakify/node_modules:/home/user/keycloakify-storyboard-storybook/node_modules/.pnpm/keycloakify@11.3.19/node_modules:/home/user/keycloakify-storyboard-storybook/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/keycloakify-storyboard-storybook/node_modules/.pnpm/keycloakify@11.3.19/node_modules/keycloakify/bin/node_modules:/home/user/keycloakify-storyboard-storybook/node_modules/.pnpm/keycloakify@11.3.19/node_modules/keycloakify/node_modules:/home/user/keycloakify-storyboard-storybook/node_modules/.pnpm/keycloakify@11.3.19/node_modules:/home/user/keycloakify-storyboard-storybook/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/main.js" "$@"
else
  exec node  "$basedir/../../bin/main.js" "$@"
fi
